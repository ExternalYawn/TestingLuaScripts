getgenv().SecureMode = true
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({
   Name = "Dragon Adventures",
   LoadingTitle = "Rayfield Interface Suite",
   LoadingSubtitle = "Bangers Lmao",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "DragonAdventures"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD.
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = true, -- Set this to true to use our key system
   KeySettings = {
      Title = "Dragon Adventures",
      Subtitle = "Key System",
      Note = "Rawr",
      FileName = "KeySystem",
      SaveKey = true,
      GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = "1234"
   }
})

local Tab = Window:CreateTab("Scripts", 4483362458)

local Section = Tab:CreateSection("Auto-Farm")

local Toggle = Tab:CreateToggle({
   Name = "Auto Farm (BETA)",
   CurrentValue = true,
   Flag = "Toggle1",
   Callback = function(Value)
   while wait() do 
   game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.Interactions.Nodes.Harvest.Eggs.ActiveNodes:GetChildren()[math.random(1,4)].Base.CFrame

task.wait(0.01)
game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("SetCollectEggRemote"):InvokeServer((math.random(3,9)))

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("CollectEggRemote"):InvokeServer((math.random(3,9)))
                                    end
                                end
})
